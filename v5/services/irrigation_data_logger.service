[Unit]
Description=Backend part of irrigation system for peregonivka for port 7542
After=network.target
Requires=network.target

[Service]
ExecStart=/usr/bin/python3 /var/www/v5/data_logger.py >> /var/log/irrigation_backend/run_data_logger.log 2>&1

RestartSec=5
TimeoutSec=5
User=root

[Install]
WantedBy=multi-user.target
